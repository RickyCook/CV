{
  "name": "RickyCook-CV",
  "version": "0.0.0",
  "description": "CV for Ricky Cook",
  "repository": {
    "type": "git",
    "url": "git://github.com/RickyCook/CV.git"
  },
  "scripts": {
    "publichtml": "./index.js public.json > public.html",
    "privatehtml": "./index.js private.json > private.html",
    "publicpdf": "phantomjs --debug=true ./pdf.js public",
    "privatepdf": "phantomjs --debug=true ./pdf.js private",
    "public": "npm run publichtml && npm run publicpdf",
    "private": "npm run privatehtml && npm run privatepdf",
    "publish": "npm run public && ./publish.sh"
  },
  "homepage": "https://github.com/RickyCook/CV",
  "dependencies": {
    "mime": "^1.3.4",
    "mustache": "^2.2.1",
    "phantomjs-prebuilt": "^2.1.7",
    "roboto-slab-fontface-kit": "^1.0.1",
    "tmp": "0.0.28",
    "url": "^0.11.0"
  }
}
